{
  "name": "nl2fofa",
  "version": "1.0.0",
  "description": "A Model Context Protocol (MCP) server that converts natural language to FOFA queries",
  "main": "dist/mcp-server.js",
  "type": "module",
  "bin": {
    "nl2fofa": "dist/index.js",
    "nl2fofa-mcp": "dist/mcp-server.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/mcp-server.js",
    "inspector": "npx @modelcontextprotocol/inspector node dist/mcp-server.js",
    "start:cli": "node dist/index.js",
    "dev": "npm run build && node dist/mcp-server.js",
    "dev:cli": "npm run build && node dist/index.js",
    "dev:watch": "tsc --watch",
    "prequery": "npm run build",
    "query": "node dist/index.js",
    "predirect": "npm run build",
    "direct": "node dist/index.js --direct",
    "prehelp": "npm run build",
    "help": "node dist/index.js --help",
    "postinstall": "npm run build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "fofa",
    "ai",
    "mcp",
    "model-context-protocol",
    "cli",
    "security",
    "search",
    "cybersecurity",
    "nlp",
    "typescript"
  ],
  "author": "King-GD",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/King-GD/NL2FOFA.git"
  },
  "homepage": "https://github.com/King-GD/NL2FOFA#readme",
  "bugs": {
    "url": "https://github.com/King-GD/NL2FOFA/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.3",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
